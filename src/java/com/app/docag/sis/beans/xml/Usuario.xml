<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.app.docag.sis.beans.java.Usuario" table="usuarios">
        <id name="id_usuario" type="java.lang.Integer" column="id_usuario">
            <generator class="sequence"> 
                <param name="sequence">usuario_id_usuario_seq</param>  
            </generator>
        </id>
        <property name="usuario"/>
        <property name="clave"/>
        <property name="nombres"/>
        <property name="apellidos"/>
        <property name="id_area"/>
        <property name="id_cargo"/>
        <property name="id_cargo_digitalizacion"/>        
        <property name="correo"/>
        <property name="id_parroquia"/>
        <property name="tlf"/>
        <property name="id_idioma"/>
        <property name="primera_vez"/>
        <property name="activo"/>        
        <property name="fecha_registro"/>
        <property name="fecha_ultima_clave"/>
        <property name="administrador"/>
        <property name="logeado"/>
        <property name="id_grupo"/>
    </class>
</hibernate-mapping>