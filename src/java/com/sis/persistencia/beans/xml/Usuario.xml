<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sis.persistencia.dao.pojos.Usuario" table="usuario">
        <id name="id_usuario" type="java.lang.Integer" column="id_usuario">
            <generator class="sequence"> 
                <param name="sequence">usuario_id_usuario_seq</param>  
            </generator>
        </id>
        <property name="usuario" type="string"/>
        <property name="password" type="string"/>
        <property name="administrador" type="string"/>
    </class>
</hibernate-mapping>